<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" width="100" height="100">
  <!-- Defining gradients for the mosque -->
  <defs>
    <!-- Gradient for the dome -->
    <radialGradient id="domeGradient3D" cx="50%" cy="50%" r="50%">
      <stop offset="0%" stop-color="#00cc44" />
      <stop offset="100%" stop-color="#004d26" />
    </radialGradient>

    <!-- Gradient for mosque body -->
    <linearGradient id="mosqueBody3D" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#00cc44" />
      <stop offset="100%" stop-color="#004d26" />
    </linearGradient>
  </defs>

  <!-- Mosque Structure -->
  <!-- Main dome with 3D gradient -->
  <path d="M35,65 Q50,40 65,65 Z" fill="url(#domeGradient3D)" />

  <!-- Minarets -->
  <rect x="40" y="50" width="5" height="15" fill="url(#domeGradient3D)" />
  <circle cx="42.5" cy="47" r="3" fill="url(#domeGradient3D)" />
  <rect x="55" y="50" width="5" height="15" fill="url(#domeGradient3D)" />
  <circle cx="57.5" cy="47" r="3" fill="url(#domeGradient3D)" />

  <!-- Mosque body -->
  <rect x="35" y="65" width="30" height="20" fill="url(#mosqueBody3D)" />
  
  <!-- Mosque entrance -->
  <rect x="45" y="70" width="10" height="15" fill="white" />
</svg>

